# Production Environment Variables for Vercel Deployment
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=production

# OTC Signal Generator Configuration
OTC_BROWSER_HEADLESS=true
OTC_MAX_PROCESSING_TIME=300000
OTC_MIN_CONFIDENCE=75
OTC_RATE_LIMIT=10
OTC_CACHE_TTL=300

# API Configuration
API_TIMEOUT=300000
API_MAX_RETRIES=3

# Logging Configuration
LOG_LEVEL=info
LOG_TO_CONSOLE=true

# Performance Configuration
MEMORY_LIMIT=1024
CPU_LIMIT=1

# Security Configuration
CORS_ORIGIN=*
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX=10